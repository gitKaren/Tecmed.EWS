@model EWS.Domain.Model.ExchangeRate
<div class="form-group">
    @Html.LabelFor(model => model.Rate, htmlAttributes: new { @class = "control-label col-md-4" })
    <div class="col-sm-6  col-md-5">
        @Html.EditorFor(model => model.Rate, new { htmlAttributes = new { @class = "form-control text-right" } })
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(model => model.Date, htmlAttributes: new { @class = "control-label col-md-4" })
    <div class="col-sm-6  col-md-5">
        @*<span class="form-control form-control-greyed text-right">*@
            @Html.EditorFor(model => model.Date, new { htmlAttributes = new { @class = "form-control form-control-greyed text-right", @readonly = "readonly" } })
        @*</span>*@
    </div>
</div>
